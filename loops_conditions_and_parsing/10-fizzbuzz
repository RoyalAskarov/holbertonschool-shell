#!/usr/bin/env bash
# This script prints numbers 1 to 100, substituting Fizz and/or Buzz for multiples of 3 and 5.

# C-style loop to iterate from 1 to 100
for ((i=1; i<=100; i++)); do
    
    # Check if divisible by BOTH 3 and 5 (15) first
    # $((i % 15)) is a common optimization for $((i % 3 == 0 && i % 5 == 0))
    if (( i % 15 == 0 )); then
        echo "FizzBuzz"
    
    # Check if divisible by 3 only
    elif (( i % 3 == 0 )); then
        echo "Fizz"
        
    # Check if divisible by 5 only
    elif (( i % 5 == 0 )); then
        echo "Buzz"
        
    # If not divisible by 3 or 5, print the number
    else
        echo "$i"
    fi
done
